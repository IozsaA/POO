package hello_world;

class Atribute {
	private int numar;
	private char caracter;

	public Atribute(int n, char c) {
		numar = n;
		caracter = c;
	}

	public void tipareste() {
		System.out.println(numar + " " + caracter);
	}
}

public class Nestatice {

	public static void main(String[] args) {
		Atribute atr = new Atribute(1, 'c');
		atr.tipareste();

	}

}
